{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "strict": true
  },
  "include": ["vite.config.ts","src/**/*.ts", "src/**/*.tsx"],
  "exclude": ["node_modules"]
}
